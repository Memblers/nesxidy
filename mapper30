-cc=vbcc6502 -I%%VBCC%%/targets/6502-nes/include -quiet -mainargs %s -o= %s %s -O=%ld
-ccv=vbcc6502 -I%%VBCC%%/targets/6502-nes/include -mainargs %s -o= %s %s -O=%ld
-unsigned-char
-as=vasm6502_oldstyle -quiet -nowarn=62 -opt-branch -Fvobj %s -o %s 
-asv=vasm6502_oldstyle -nowarn=62 -Fvobj -opt-branch -v %s -o %s
-rm=del %s
-rmv=del %s
-cpr=vcpr6502 -quiet %s %s
-cprv=vcpr6502 %s %s
-ld=vlink -vicelabels vicemap.map -b rawbin1 -Cvbcc -Tmapper30.cmd -L%%VBCC%%/targets/6502-nes/lib %%VBCC%%/targets/6502-nes/lib/startup.o %s %s -o %s -lvc
-ldv=vlink -vicelabels vicemap.map -v -b rawbin1 -Cvbcc -Tmapper30.cmd -L%%VBCC%%/targets/6502-nes/lib %%VBCC%%/targets/6502-nes/lib/startup.o %s %s  -o %s -lvc
